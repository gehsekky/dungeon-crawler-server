{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "globals": {
        "CONFIG": false
    },
    "rules": {
        "no-console": 0,
        "linebreak-style": 0,
        "import/no-unresolved": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-one-expression-per-line": 0,
        "react/prefer-stateless-function": 0,
        "react/forbid-prop-types": 0
    }
}